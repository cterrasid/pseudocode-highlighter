{
  "Algorithm Template": {
    "prefix": "algorithm",
    "body": [
      "algorithm ${1:AlgorithmName}",
      "    var",
      "        ${2:variables}",
      "    end var",
      "",
      "    {Code logic here}",
      "end algorithm"
    ],
    "description": "Basic template for an algorithm"
  },
  "If-Else Template": {
    "prefix": "if",
    "body": [
      "if ${1:condition} then",
      "    ${2:statements}",
      "else",
      "    ${3:statements}",
      "end if"
    ],
    "description": "Template for if-else structure"
  },
  "While-Do Template": {
    "prefix": "while",
    "body": [
      "while ${1:condition} do",
      "    ${2:statements}",
      "end while"
    ],
    "description": "Template for while-do cycle"
  },
  "For-To-Do Template": {
    "prefix": "for",
    "body": [
      "for ${1:condition} to ${2:condition} do",
      "    ${3:statements}",
      "end for"
    ],
    "description": "Template for for-to-do cycle"
  },
  "Variable Template": {
    "prefix": "var",
    "body": [
      "var",
      "    ${1:variables}",
      "end var"
    ],
    "description": "Template for variables creation"
  }
}
